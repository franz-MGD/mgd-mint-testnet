{
  "extends": [
    "plugin:prettier/recommended",
    "next/core-web-vitals"
  ],
  "plugins": [
    "baseui"
  ],
  "rules": {
    "semi": [
      "error",
      "always"
    ],
    "quotes": [
      "error",
      "single"
    ],
    "baseui/deprecated-theme-api": "warn",
    "baseui/deprecated-component-api": "warn",
    "baseui/no-deep-imports": "warn"
  }
}